{
  "log_params": {
    "log_directory": "logs",
    "log_info": "info.log",
    "log_error": "error.log",
    "log_warn": "warn.log",
    "log_debug": "debug.log",
    "max_size_megabytes": 10,
    "max_backups": 4,
    "max_age": 30,
    "compress": true,
    "local_time": true
  },
  "app_params": {
    "server_url": "localhost",
    "server_name": "postgres",
    "app_version": "1.8",
    "port_run": "8181",
    "gin_mode": "debug"
  },
  "postgres_params": {
    "user": "postgres",
    "host": "localhost",
    "port": "5432",
    "database": "postgres"
  },
  "auth_params": {
    "jwt_secret_key": "secret_key",
    "jwt_ttl_minutes": 60
  }
}